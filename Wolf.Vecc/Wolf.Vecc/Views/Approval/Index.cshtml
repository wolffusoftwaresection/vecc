                                                }
                                                return ck;
                                            }
                                        }, {
                                            field: 'DataUrl',
                                            align: 'center',
                                            title: '下载',
                                            visible: 1,
                                            formatter: function (value, row, index) {
                                                var ck = '<a href="' + value + '"><span class= "glyphicon glyphicon-cloud-download" ></span></a>';
                                                return ck;
                                            }
                                        }
