                                var _msg = msg;
                                var checkedbox = $("#tb_DataApproval").bootstrapTable('getSelections');
                                //将选中行数据转成jsonStr
                                var jsonStr = JSON.stringify(checkedbox);
                                var jsonObject = jQuery.parseJSON(jsonStr);
                                if (jsonObject.length > 0) {
                                    _msg.confirm("是否执行此操作!", function () {
                                        var zeo = [];
                                        for (var i = 0; i < jsonObject.length; i++) {
                                            var jValue = jsonObject[i]["Id"];//key所对应的value
                                            zeo.push(jValue);
                                        }
                                        var ids = zeo.join(",");
                                        $.ajax({
                                            url: "/Approval/BatchAgreeData",
                                            data: {
                                                Ids: ids
                                            },
                                            type: "POST",
                                            async: false,
                                            success: function (data) {
                                                if (data.Success) {
                                                    screeningSearch1();
                                                    msg.info("执行完成!");
                                                }
                                            },
                                            error: function (data) {
                                            }
                                        });
                                    });
                                }
                                else {
                                    return msg.info("请选择操作数据!");
                                }
                            }

